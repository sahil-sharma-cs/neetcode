public static int maxProfit(int[] prices) {

        int profit = 0;
        int start = 0;
        int min = prices[0];
        for(int i=1; i<prices.length; i++) {
            if(prices[i] - prices[start] > 0) {
                profit = Math.max(profit, prices[i] - prices[start]);
            }
            if(prices[i] < min) {
                start = i;
                min = prices[i];
            }
        }
        return profit;
    }
